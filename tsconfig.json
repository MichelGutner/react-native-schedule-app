{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["jest"],
    "plugins": [
      {"transform": "typescript-transform-paths"},
      {"transform": "typescript-transform-paths", "afterDeclarations": true}
    ]
  }
}
